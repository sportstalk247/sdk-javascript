{
  "name": "sportstalk-sdk",
  "version": "3.5.8",
  "description": "Javascript SDK for SportsTalk",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "nyc mocha -r ts-node/register test/integration/**/*.spec.ts  --timeout 10000",
    "test:room": "nyc mocha -r ts-node/register test/integration/chat/pre-moderation.spec.ts --timeout 5000",
    "build": "tsc --declaration",
    "web": "tsc --declaration && browserify ./dist/web.js -o ./dist/web-sdk.js",
    "web:prod": "tsc --declaration && browserify ./dist/web.js -p tinyify -o ./dist/web-sdk.min.js"
  },
  "author": "Sportstalk24/7",
  "license": "LicenseRef-LICENSE",
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/browserify": "^12.0.36",
    "@types/chai": "^4.2.7",
    "@types/expect": "^24.3.0",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.12.20",
    "awesome-typescript-loader": "^5.2.1",
    "axios-mock-adapter": "^1.17.0",
    "browserify": "^16.5.0",
    "chai": "^4.2.0",
    "deep-equal-in-any-order": "^1.0.21",
    "dotenv": "^8.2.0",
    "mocha": "^6.2.2",
    "nyc": "^14.1.1",
    "sinon": "^8.1.0",
    "source-map-loader": "^0.2.4",
    "source-map-support": "^0.5.16",
    "tinyify": "^2.5.2",
    "ts-mocha": "^6.0.0",
    "ts-node": "^8.5.4",
    "typescript": "^3.7.3"
  },
  "dependencies": {
    "axios": "^0.19.0"
  },
  "files": [
    "/src",
    "/dist",
    "/lib"
  ],
  "nyc": {
    "include": [
      "src/impl/**/*.ts",
      "src/**/*.tsx"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ],
    "sourceMap": true,
    "instrument": true,
    "all": true
  }
}
